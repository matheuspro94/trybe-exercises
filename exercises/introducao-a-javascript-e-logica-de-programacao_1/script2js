// Prática parte 2 - Faça um programa que retorne o maior de dois números. Defina no começo do programa duas variáveis com os valores que serão comparados.

let a = 4;
let b = 6;
if (a > b) {
  console.log('A é maior')
} else if (b > a ){
  console.log('B é maior')
} else {
  console.log('Os numeros são iguais')
}

